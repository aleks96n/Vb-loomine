<hi>Log in</hi>
<div class = "row">
  <div class = "col-md-6 col-md-offset-3">
    <%= form_for(:session, url: login_path) do |f|%>
      <%= f.label :email%>
      <%= f.email_field :email, class: 'from-control' %>

      <%= f.label :password%>
      <%= f.password_field :password, class: 'from-control' %>

      <%= f.submit "Log in", class: "btn btn-primary" %>
    <% end %>

  </div>
</div>
